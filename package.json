{
  "name": "portfolio",
  "private": true,
  "version": "0.2.0",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier . --write",
    "count": "git ls-files | xargs cat | wc -l",
